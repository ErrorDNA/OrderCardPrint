<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="LocalDarkGreen" Color="#01A077" />
    <SolidColorBrush x:Key="LocalLightGreen" Color="#E5F3F3" />
    <LinearGradientBrush x:Key="LocalDarkToLightGreen" StartPoint="0,0" EndPoint="1,0" >
        <GradientStop Color="#B9DED6" Offset="0" />
        <GradientStop Color="#E5F3F3" Offset="1.0" />
    </LinearGradientBrush>
</ResourceDictionary>